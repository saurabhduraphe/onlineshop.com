<script> 
    var productSearch = [];
    var csrfMetaTag = document.querySelector('meta[name="csrf_token"]');
    $.ajax({ 
        url: "ajaxData.php",
        //                async:false,
        type: "post", //The type which you want to use: GET/POST
        data: "searchProduct=search"+"&&csrf_token="+csrfMetaTag.getAttribute('content'),
        dataType: "json", //Return data type (what we expect).
        //This is the function which will be called if ajax call is successful.
        success: function(data) {  
            //console.log(data);
            //                    $('#serachResult').html(data);
            productSearch = data;
            autocompleteFn();
        }
    });

    function autocompleteFn() {
        //                alert("hie");
        $("#searchInput").autocomplete({
            source: productSearch,
            appendTo: '#search-field',
            classes: {
                "ui-autocomplete": "auto-search"
            },
            //                source: "getSerach.php",
            minLength: 0,
            select: function(event, ui) {
                console.log(ui.item.id);
                $("#searchInput").val(ui.item.value);
                $("#userID").val(ui.item.id);
                var str = ui.item.value;
                str = str.replace(/\s+/g, '-').toLowerCase();
                // window.location.href = "product_details.php?id=" + ui.item.id;
                window.location.href = "products/"+ str + "/" + ui.item.id;
            },
            //                    focus: function( event, ui ) {
            //                        console.log(ui.item.label);
            //                        $( "#searchInput" ).val( ui.item.value );
            //                        return false;
            //                    },
        }).focus(function() {
            if ($(this).val() != "") {
                $(this).autocomplete('search', $(this).val())
            }

        }).data("ui-autocomplete")._renderItem = function(ul, item) {
                        //    console.log(ul,'//',item);
            return $("<li class='ui-autocomplete-row'></li>")
                .data("item.autocomplete", item)
                .append(item.label)
                .appendTo(ul);

        };
    }

    //search click button header
    $('#searchForm123').on('submit', function(e) {
        e.preventDefault(); // cancel default action
        console.log("huie" + $('#searchInput').val());
        var input = $('#searchInput').val().replace(/ /g,'-');
        // var input = $.trim(input);
        //                window.location.href = "shop.php?catid=4"; 
        window.location.href = "search/" + input;
        return false;
    });
</script>



